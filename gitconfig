  email = filip@tepper.pl
[color]
  diff = auto
  status = auto
  branch = auto
[push]
  default = current
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
  prune = fetch --prune
  undo = reset --soft HEAD^
  stash-all = stash save --include-untracked
[core]
