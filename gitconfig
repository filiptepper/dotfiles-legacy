[user]
	name = Filip Tepper
	email = filip@tepper.pl
[color]
  diff = auto
  status = auto
  branch = auto
[push]
  default = current
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[alias]
  prune = fetch --prune
  undo = reset --soft HEAD^
  stash-all = stash save --include-untracked
[core]
